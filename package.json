{
  "name": "relabeler-action",
  "version": "0.0.0",
  "private": true,
  "description": "Relabeler action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc --newLine lf",
    "build-watch": "tsc --watch --sourceMap --newLine lf",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint **/*.ts && npm run format-check",
    "package": "ncc build --source-map --no-cache --license licenses.txt",
    "test": "jest --coverage",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage",
    "all": "npm run build && npm run package && npm run lint && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0"
  },
  "devDependencies": {
    "@octokit/webhooks-types": "^7.5.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.3",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "prettier": "3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5"
  }
}
